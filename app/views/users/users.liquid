<h1>All Users</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Gender</th>
      <th>Age</th>
      <th>Created At</th>
    </tr>
  </thead>
  <tbody>
    {% for user in users %}
      <tr>
        <td>{{ user.full_name }}</td>
        <td>{{ user.gender&.capitalize }}</td>
        <td>{{ user.age }}</td>
        <td>{{ user.created_at.httpdate }}</td>
      </tr>
    {% endfor %}
  </tbody>
</table>
